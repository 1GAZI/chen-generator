{
  "name": "acmTemplate-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.chen",
  "version": "1.0",
  "author": "chen",
  "createTime": "2024-4-14",
  "fileConfig": {
    "inputRootPath": ".source/acmTemplate-pro",
    "outputRootPath": "generated",
    "sourceRootPath": "D:/java/code/chen-generator/demoProjects/acmTemplate-pro",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/chen/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/chen/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "chen",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  }
}